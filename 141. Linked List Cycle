# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def hasCycle(self, head):
        """
        :type head: ListNode
        :rtype: bool
        """
        current = head
        second = head
        while(current and second and second.next):
            current = current.next
            second = second.next.next
            if(current == second):
                return True
        return False
        
